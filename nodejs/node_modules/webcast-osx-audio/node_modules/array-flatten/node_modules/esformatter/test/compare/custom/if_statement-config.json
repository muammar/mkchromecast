{
    "lineBreak" : {
        "before" : {
            "IfStatement": ">=1",
            "ElseStatement" : 1,
            "ElseIfStatement" : 1,
            "IfStatementOpeningBrace" : 1,
            "ElseStatementOpeningBrace" : 1,
            "ElseIfStatementOpeningBrace" : 1,
            "LineComment": -1
        },
        "after": {
            "IfStatementOpeningBrace" : 1,
            "ElseStatementOpeningBrace" : 1
        }
    },

    "whiteSpace" : {
        "before" : {
            "IfStatementConditionalOpening" : 0,
            "IfStatementConditionalClosing" : 0
        },
        "after" : {
            "IfStatementConditionalOpening" : 0,
            "IfStatementConditionalClosing" : 1
        }
    }
}
